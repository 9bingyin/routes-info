{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://mirror.ghproxy.com/https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "direct",
      "default_mode": "rule"
    },
  "cache_file": {
    "enabled": true,
    "store_fakeip": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "proxyDns",
        "address": "https://1.1.1.1/dns-query",
        "detour": "代理规则"
      },
      {
        "tag": "localDns",
        "address": "https://223.5.5.5/dns-query",
        "detour": "DIRECT"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "localDns",
        "disable_cache": true
      },
      {
        "rule_set": "geosite-cn",
        "server": "localDns"
      },   
      {
        "clash_mode": "direct",
        "server": "localDns"
      },
      {
        "clash_mode": "global",
        "server": "proxyDns"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "proxyDns"
      }
    ],
    "final": "localDns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "mtu": 1500,
      "auto_route": true,
      "strict_route": false,
      "sniff": true,
      "endpoint_independent_nat": false,
      "stack": "mixed",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],
  "outbounds": [
    {
      "tag":"代理规则",
      "type":"selector",
      "outbounds":[
        "自动选择",
        "DIRECT",
        "{all}"
      ]
    },
    {
      "tag":"自动选择",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"exclude","keywords":["网站|地址|剩余|过期|时间|有效"]}
      ],
      "url": "http://detectportal.firefox.com/success.txt",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag":"巴哈姆特",
      "type":"selector",
      "outbounds":[
        "代理规则",
        "DIRECT",
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["Taiwan"]}
      ]
    },
    {
      "tag":"ABEMA",
      "type":"selector",
      "outbounds":[
        "代理规则",
        "DIRECT",
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["Japan"]}
      ]
    },
    {
      "tag":"直连规则",
      "type":"selector",
      "outbounds":[
        "DIRECT",
        "代理规则",
        "自动选择"
      ]
    },
    {
      "tag":"默认规则",
      "type":"selector",
      "outbounds":[
        "代理规则",
        "DIRECT",
        "自动选择"
      ]
    },
    {
      "type": "direct",
      "tag": "DIRECT"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "代理规则",
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "clash_mode": "direct",
        "outbound": "DIRECT"
      },
      {
        "clash_mode": "global",
        "outbound": "代理规则"
      },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "DIRECT"
      },
      {
        "rule_set": "geosite-bahamut",
        "outbound": "巴哈姆特"
      },
      {
        "rule_set": "geosite-abema",
        "outbound": "ABEMA"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "默认规则"
      },
      {
        "rule_set": "geosite-private",
        "outbound": "DIRECT"
      },
      {
        "ip_is_private": true,
        "outbound": "DIRECT"
      },
      {
        "rule_set": "geoip-cn",
        "outbound": "直连规则"
      },
      {
        "rule_set": "geosite-cn",
        "outbound": "直连规则"
      }
    ],
    "rule_set": [
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "download_detour": "代理规则"
      },
      {
        "tag": "geosite-private",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs",
        "download_detour": "代理规则"
      },
      {
        "tag": "geosite-bahamut",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/bahamut.srs",
        "download_detour": "代理规则"
      },
      {
        "tag": "geosite-abema",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/abema.srs",
        "download_detour": "代理规则"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs",
        "download_detour": "代理规则"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://gcore.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "代理规则"
      }
    ]
  }
}
